  "bugs": {
    "url": "https://github.com/JDGonzal/cypress-training/issues"
  },
  "homepage": "https://github.com/JDGonzal/cypress-training#readme",
  "dependencies": {
    "cypress": "^10.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^2.12.1",
    "typescript": "^4.7.4"
